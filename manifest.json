{
  "manifest_version": 3,
  "name": "Youtube progress bar back to red",
  "version": "1.1",
  "description": "A Chrome extension to change the background color of YouTube progress bar back to red.",
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["myStyles.css"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
