{
  "manifest_version": 3,
  "name": "Youtube progress bar back to red",
  "version": "1.2",
  "description": "A Chrome extension to change the background color of YouTube progress bar back to red.",
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["myStyles.css"],
      "js": ["Content.js"],
      "__COMMENT__": "https://www.reddit.com/r/webdev/comments/bjhc2z/comment/lf4iq91/",
      "all_frames": true
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "eleodor_k9@yahoo.com",
      "strict_min_version": "109.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
